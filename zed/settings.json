{
    "theme": "Terafox",
    "base_keymap": "VSCode",
    "buffer_font_family": "JetBrainsMono Nerd Font Mono",
    "buffer_font_size": 15,
    "vim_mode": true,
    "relative_line_numbers": true,
    "cursor_blink": false,
    "scrollbar": {
        "show": "never"
    },
    "buffer_line_height": {
        "custom": 1.7
    },
    "terminal": {
        "font_family": "JetBrainsMono Nerd Font Mono",
        "font_size": 15,
        "line_height": {
            "custom": 1.7
        }
    },
    "inlay_hints": {
        "enabled": false
    },
    "lsp": {
        "java": {
            "settings": {
                "java.jdt.ls.lombokSupport.enabled:": true
            }
        }
    },
    "auto_install_extensions": {
        "git-firefly": true,
        "nightfox-themes": true,
        "toml": true,
        "java": true
    },
    "assistant": {
        "default_model": {
            "provider": "copilot_chat",
            "model": "claude-3-5-sonnet"
        },
        "version": "2"
    }
}
